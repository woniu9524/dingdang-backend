<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.dingdang.mapper.AuthMapper">

    <!-- 插入操作 -->
    <insert id="register" parameterType="org.dingdang.domain.SysUser">
        INSERT INTO sys_user (user_id, open_id, username)
        VALUES (#{userId}, #{openId}, #{userName})
    </insert>

</mapper>
